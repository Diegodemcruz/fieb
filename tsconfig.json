{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": ["styled-components-react-native"],
    "baseUrl": ".",
    "paths": {
      "@screens/*": ["screens/*"],
      "@components/*": ["components/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@stores": ["stores/index.ts", "stores/index.tsx"],
      "@stores/*": ["stores/*"],
      "@routes": ["routes/index.ts", "routes/index.tsx"],
      "@routes/*": ["routes/*"],
      "@services/*": ["services/*"]
    }
  }
}
