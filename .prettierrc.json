{
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "useTabs": true,
  "printWidth": 100,
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "importOrder": [
    "^react$",
    "^expo(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@assets/(.*)$",
    "^@components/(.*)$",
    "^@env(.*)$",
    "^@routes/(.*)$",
    "^@screens/(.*)$",
    "^@services/(.*)$",
    "^@stores/(.*)$",
    "^@utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}